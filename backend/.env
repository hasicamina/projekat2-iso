# Server konfiguracija
PORT=3000
NODE_ENV=production

# PostgreSQL konfiguracija - ove vrijednosti će biti zamijenjene u runtime
DB_HOST=${DB_HOST}
DB_PORT=5432
DB_NAME=${DB_NAME}
DB_USER=${DB_USER}
DB_PASSWORD=${DB_PASSWORD}

# DATABASE_URL će biti konstruisan iz gornjih vrijednosti
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:5432/${DB_NAME}